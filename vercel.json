{
  "functions": {
    "api/advance.py": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "crons": [
    {
      "path": "/api/advance",
      "schedule": "*/5 * * * *"
    }
  ]
}
